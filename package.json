		"extract-text-webpack-plugin": "^3.0.2",
		"html-minifier": "^3.5.7",
		"imagemin-webpack-plugin": "^1.5.2",
		"jest": "^21.2.1",
		"jest-cli": "^21.2.1",
		"jsonminify": "^0.4.1",
		"node-sass": "^4.7.2",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"react-hot-loader": "^3.1.3",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0",
		"uglifyjs-webpack-plugin": "^1.1.2",
