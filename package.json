	"jest": {
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleNameMapper": {
			"^react$": "<rootDir>/node_modules/react",
			"^react-dom$": "<rootDir>/node_modules/react-dom"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!redux-ducklings)/"
		],
		"setupTestFrameworkScriptFile": "<rootDir>/tests-config/setup.js"
	}
}
