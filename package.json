	"main": "index.js",
	"scripts": {
		"dev": "webpack-dev-server --hot --history-api-fallback --env.hot=true --open",
		"watch": "webpack --progress --watch",
		"build": "webpack -p",
		"analyze": "webpack -p --env.analyze=true",
		"lint": "\"./node_modules/.bin/eslint\" src",
		"lint-fix": "\"./node_modules/.bin/eslint\" src --fix",
